<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
        android:color="@color/highlightPressed">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="5px" android:color="@color/colorButtonNotPressed" />
            <corners android:radius="24dp"/>
            <solid android:color="@android:color/transparent"/>
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"/>
        </shape>
    </item>
</ripple>