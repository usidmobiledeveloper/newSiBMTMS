<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableTextView">
        <attr name="trimLength" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TicketView">
        <attr format="reference" name="tv_anchor1"/>
        <attr format="reference" name="tv_anchor2"/>

        <attr format="color" name="tv_dashColor"/>
        <attr format="dimension" name="tv_dashSize"/>
        <attr format="dimension" name="tv_circleSpace"/>
        <attr format="dimension" name="tv_circleRadius"/>
    </declare-styleable>

    <declare-styleable name="ZigzagView">
        <attr name="zigzagHeight" format="dimension" />
        <attr name="zigzagElevation" format="dimension" />
        <attr name="zigzagBackgroundColor" format="color" />
        <attr name="zigzagPaddingContent" format="dimension" />
        <attr name="zigzagPadding" format="dimension" />
        <attr name="zigzagPaddingLeft" format="dimension" />
        <attr name="zigzagPaddingRight" format="dimension" />
        <attr name="zigzagPaddingBottom" format="dimension" />
        <attr name="zigzagPaddingTop" format="dimension" />
        <attr name="zigzagSides">
            <flag name="top" value="1" />
            <flag name="bottom" value="2" />
        </attr>
        <attr name="zigzagShadowAlpha" format="float" />
    </declare-styleable>
</resources>